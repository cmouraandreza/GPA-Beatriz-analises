#pre-processamento -----


# recuperando tabela utilizada
dados<-readxl::read_xlsx("Data/COPIA-Database_epizoism_hydroids-bea.xlsx")


str(dados)

#dados_crus<-readxl::read_xlsx("atributos.xlsx", sheet=3)



colnames(dados)


teste.disp<- dados%>%
  select("Comp.total","Substrato","Arranjo.eixo" ,"N.Anel","PadrÃ£o.hidroteca","PadrÃ£o.ramif", "BD.eixo",
         "C.perissarco","H.perissarco" , "G.perissarco" ,"Ciclo.vida","Colonialismo" ,"ReproduÃ§Ã£o" ,"Salinidade","Temperatura"   )



dados_filtrados <- teste.disp %>%
  filter(Temperatura >= 0 & Temperatura<= 30)



#sumarizando os dados 
# verificando 
summary(dados_filtrados$Comp.total)



# colocando dados de charcter para numeric
#dados_filtrados <- dados_filtrados %>%
#  mutate(across(where(is.character), as.numeric))
# joga tudo como numerico

# coloca uma coluna como num
# Se sua coluna estÃ¡ em um data frame chamado 'dados'
dados_filtrados$Comp.total <- as.numeric(dados_filtrados$Comp.total)

library(dplyr)
dados_filtrados1 <- dados_filtrados %>%
  mutate_if(is.character, as.factor)



dados_filtrados1$BD.eixo <- as.numeric(gsub(",", ".", dados_filtrados1$BD.eixo))
str(dados_filtrados1)
# Primeiro, certifique-se que dados_filtrados Ã© um data frame
#dados_filtrados <- dados_filtrados %>%
# mutate(Substrate = as.factor(Substrate))  # ou mantenha como character se preferir

# RETIRANDO na
dados_filtrados1 <- dados_filtrados1 %>%
  filter(!is.na(Comp.total))

# filtrando os outliers
# Filtra as linhas onde x < 15000
dados_filtrados1 <- dados_filtrados1 %>%
  filter(Comp.total < 15.000)

#dados_filtrados <- dados_filtrados %>%

dados_filtrados2 <- dados_filtrados1[dados_filtrados1$Salinidade >= 32.5, ]
dados_filtrados3 <- dados_filtrados2[dados_filtrados2$Comp.total <= 10.00, ]
str(dados_filtrados3)

dados_filtrados3 <- dados_filtrados3 %>% filter(!is.na(Salinidade_Classe))


# ğŸ“Œ Instalar e carregar pacotes necessÃ¡rios ----
#install.packages(c("car", "ggplot2", "dplyr"))
library(car)      # Para o teste de Levene
library(ggplot2)  # Para visualizaÃ§Ã£o dos dados
library(dplyr)    # Para manipulaÃ§Ã£o de dados

# ğŸ“Œ Carregar os dados (substituir pelo seu dataset)
# Exemplo: Suponha que temos uma variÃ¡vel categÃ³rica "Grupo" e duas variÃ¡veis contÃ­nuas "Var1" e "Var2"

# ğŸ“Œ Teste de normalidade (Shapiro-Wilk) para cada variÃ¡vel contÃ­nua dentro de cada grupo
by(dados_filtrados3$Salinidade, dados_filtrados3$Substrato, shapiro.test)
by(dados_filtrados3$Temperatura, dados_filtrados3$Substrato, shapiro.test)

# ğŸ“Œ Teste de homogeneidade das variÃ¢ncias (Levene)
leveneTest(Salinidade ~ Substrato, data = dados_filtrados3)  # Para Var1
leveneTest(Temperatura ~ Substrato, data = dados_filtrados3)  # Para Var2

# ğŸ“Œ Escolher o teste adequado com base nos resultados
# Se normalidade e homocedasticidade forem confirmadas â†’ ANOVA padrÃ£o
# Se variÃ¢ncias forem diferentes â†’ ANOVA de Welch
# Se os dados nÃ£o forem normais â†’ Kruskal-Wallis

# ğŸ“Œ ANOVA padrÃ£o (se as premissas forem atendidas)
anova1 <- aov(Salinidade ~ Substrato, data = dados_filtrados3)
anova2 <- aov(Temperatura ~ Substrato, data = dados_filtrados3)
summary(anova1)
summary(anova2)

# ğŸ“Œ ANOVA de Welch (se variÃ¢ncias forem diferentes)
oneway.test(Salinidade ~ Substrato, data = dados_filtrados3, var.equal = FALSE)
oneway.test(Temperatura ~ Substrato, data = dados_filtrados3, var.equal = FALSE)

# ğŸ“Œ Teste de Kruskal-Wallis (se os dados nÃ£o forem normais)
kruskal.test(Var1 ~ Grupo, data = dados)
kruskal.test(Var2 ~ Grupo, data = dados)

# ğŸ“Œ Se a ANOVA ou Kruskal-Wallis forem significativas, realizar comparaÃ§Ãµes mÃºltiplas:
# Para ANOVA: Teste de Tukey
TukeyHSD(anova1)
TukeyHSD(anova2)

# Para Kruskal-Wallis: Teste de Dunn (usando PMCMRplus)
install.packages("PMCMRplus")
library(PMCMRplus)
dunn.test <- kwAllPairsDunnTest(Var1 ~ Grupo, data = dados)
print(d)


