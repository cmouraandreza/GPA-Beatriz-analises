# ğŸ“Œ Criar o grÃ¡fico de dispersÃ£o com linha de regressÃ£o
a<-ggplot(dados_filtrados3, aes(x = Temperatura, y = Comp.total)) +
  geom_point( color= "blue", size= 3, alpha = 0.6) +  # Pontos azuis semi-transparente
  geom_smooth(method = "lm", color = "red", se = TRUE) +  # Linha de regressÃ£o em vermelho
  labs(
    x = "Temperatura (Â°C)",
    y = "Comprimento da ColÃ´nia (cm)"
  ) +
  theme(
    panel.grid = element_blank(),
    panel.border = element_rect(color = "black", fill = NA),
    panel.background = element_rect(fill = "white"),  # fundo branco
    legend.position = "right",  # legenda horizontal embaixo
    legend.direction = "vertical",
    
    plot.background = element_rect(fill = "white")    # fundo do plot branco
  )+
  annotate("text", x = 1, y = 11.5, label = "(a)",
           fontface = "bold", size=5)
