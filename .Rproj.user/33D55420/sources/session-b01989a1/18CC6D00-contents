  theme(axis.text.x = element_text(angle = 90, hjust = 1)) + # Rotaciona os rÃ³tulos do eixo X
  annotate("text", x = 1, y = 3.4, label = "(j)",
           fontface = "bold", size=5)
j 

# plot final ğŸ“Œ -----
p<-(a+b)/(c+d)/ (e+f)/(g+h)/(i+j)+ 
  plot_layout(guides = "collect") +
  plot_annotation(theme = theme(legend.position = "right"))

