# ğŸ“Œ Criar classes de salinidade sem lacunas
dados_filtrados3$Salinidade_Classe <- cut(
  dados_filtrados3$Salinidade, 
  breaks = seq(floor(min(dados_filtrados3$Salinidade)), 
               ceiling(max(dados_filtrados3$Salinidade)), 
               by = 2),  # Ajuste o intervalo (by) para garantir cobertura
  include.lowest = TRUE, 
  right = FALSE  # Intervalos Ã  esquerda fechados para evitar buracos
)

# ğŸ“Œ Criar classes de temperatura sem lacunas
dados_filtrados3$Temperatura_Classe <- cut(
  dados_filtrados3$Temperatura, 
  breaks = seq(floor(min(dados_filtrados3$Temperatura)), 
               ceiling(max(dados_filtrados3$Temperatura)), 
               by = 5),  # Ajuste o intervalo conforme necessÃ¡rio
  include.lowest = TRUE, 
  right = FALSE
)

# ğŸ“Œ Verificar se todas as classes foram preenchidas corretamente
table(dados_filtrados3$Salinidade_Classe)
table(dados_filtrados3$Temperatura_Classe)



# ğŸ“Œ Calcular proporÃ§Ã£o dentro de cada classe de salinidade
dados_summary <- dados_filtrados3 %>%
  group_by(Temperatura_Classe, Substrato) %>%
  summarise(Contagem = n(), .groups = "drop") %>%
  group_by(Temperatura_Classe) %>%
  mutate(Proporcao = Contagem / sum(Contagem) * 100)

# grafico 
# ğŸ“Œ Criar o grÃ¡fico de barras empilhadas
a<-ggplot(dados_summary, aes(x = Proporcao, y = Temperatura_Classe,  fill = Substrato)) +
  geom_bar(stat = "identity", position = "stack", alpha = 0.6) +  # Empilhamento padrÃ£o
  labs(title = "Temperatura(ÂºC)",
       y = "Temperatura (ÂºC)",
       fill = "(a/b) Substrato") +
  theme(
    axis.title.x = element_blank(), #retira o label para nao ficar repetitivo
    axis.title.y = element_blank(), #retira o label para nao ficar repetitivo
    
    panel.grid = element_blank(),
    panel.border = element_rect(color = "black", fill = NA),
    panel.background = element_rect(fill = "white"),  # fundo branco
    
    # Adicionar um retÃ¢ngulo branco atrÃ¡s do "(a)"
    legend.position = "right",  # legenda horizontal embaixo
    plot.background = element_rect(fill = "white")    # fundo do plot branco
  ) +
  scale_fill_manual(values = c("lightblue", "#ff6d00","gold", "black")) +  # Definir cores diferentes para os ciclos
  theme(axis.text.x = element_text(angle = 90, hjust = 1)) + # Rotaciona os rÃ³tulos do eixo X
  annotate("text", x = 1, y = 6.2, label = "(a)",
           fontface = "bold", size=5)

a
